{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "format-check": "prettier --check '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "lint": "next lint --dir src --dir stories --dir .storybook --dir tests --dir scripts --dir app --dir lib --dir types",
    "lint-fix": "npm run lint -- --fix",
    "postinstall": "node ./scripts/postinstall.js",
    "start": "next start -p ${PORT:-3000}",
    "storybook": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "test": "jest --ci --coverage",
    "test-update": "jest --update-snapshot",
    "test-watch": "jest --watch",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@trussworks/react-uswds": "^9.0.0",
    "@uswds/uswds": "3.8.1",
    "lodash": "^4.17.21",
    "next": "^14.0.3",
    "next-intl": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.5",
    "@types/jest-axe": "^3.5.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.13.0",
    "jest": "^29.5.0",
    "jest-axe": "^8.0.0",
    "jest-cli": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.0.0",
    "postcss-preset-env": "^9.0.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "sass-loader": "^14.0.0",
    "storybook": "^8.0.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
